{
    "name": "@kpsys/angularjs-value-editor",
    "version": "0.0.1",
    "author": {
        "email": "tomas.dostal@kpsys.cz",
        "name": "Tomas Dostal"
    },
    "description": "Dynamically created inputs for AngularJS",
    "repository": "https://github.com/DostalTomas/angularjs-value-editor.git",
    "dependencies": {
        "angular": "latest",
        "angularjs-register": "github:DostalTomas/angularjs-register",
        "@kpsys/angular-ui-bootstrap": "latest"
    },
    "main": "dist/value-editor.js",
    "style": "dist/value-editor.css",
    "license": "MIT",
    "publishConfig": {
        "access": "public"
    },
    "directories": {
        "lib": "src/"
    },
    "files": [
        "demo/",
        "dist/",
        "src/",
        "webpack.demo.config.js",
        "webpack.module.config.js",
        "tsconfig.json"
    ],
    "devDependencies": {
        "@babel/core": "latest",
        "@types/angular": "latest",
        "@types/node": "latest",
        "babel-loader": "next",
        "babel-plugin-angularjs-annotate": "latest",
        "babel-preset-env": "next",
        "html-loader": "latest",
        "clean-webpack-plugin": "latest",
        "css-loader": "latest",
        "extract-text-webpack-plugin": "next",
        "less": "latest",
        "less-loader": "latest",
        "pug-html-loader": "latest",
        "source-map-loader": "^0.2.4",
        "style-loader": "latest",
        "ngtemplate-loader": "latest",
        "ts-loader": "latest",
        "tslint": "latest",
        "tslint-loader": "latest",
        "typescript": "latest",
        "webpack": "latest",
        "webpack-cli": "latest",
        "webpack-dev-server": "latest",
        "html-webpack-plugin": "latest"
    },
    "scripts": {
        "lib:prod": "webpack --config webpack.module.config.js --mode production --progress",
        "lib:devel": "webpack --config webpack.module.config.js --mode development --watch --progress",
        "demo": "webpack-dev-server --config webpack.demo.config.js --progress"
    }
}
