main.container(ng-controller="labController as $ctrl")

    form(name="$ctrl.form" novalidate="novalidate")

        kp-value-editor(
            type="'acceptable-root'"

            options="$ctrl.options"

            validations="$ctrl.validations"

            ng-model="$ctrl.model"
        )

    div Model:
    pre {{$ctrl.model | json}}

    div Form:
    pre {{$ctrl.form | json}}

